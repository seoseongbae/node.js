<html>
    <head>
        <meta charset="UTF-8">
        <title>출력결과</title>
    </head>
    <body>
        <script>
            function solution(s) {
                let answer=0;
                for(let i of s)
                {
                   /*let num=i.charCodeAt();
                   if(num>=65&&num<=90) answer++;*/ //이런식의 조건문 또는
                   if(i===i.toUpperCase()) answer++;
                }

                return answer;
            }
            let str="KoreaTimeGood";
           console.log(solution(str));
           
        </script>
    </body>
</html>

