<html>
    <head>
        <meta charset="UTF-8">
        <title>출력결과</title>
    </head>
    <body>
        <script>
             function solution(s){
                let answer=s;
                answer=s.replace(/A/g,'#'); // replace = 바꾼다 , 그 괄호안에 g를 넣으면 모든 값을 다 바꿔주는 것.
                return answer;
            }

            let str="BANANA";
            console.log(solution(str));

        </script>
    </body>
</html>
